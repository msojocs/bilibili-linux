{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug Main Process",
          "type": "node",
          "request": "launch",
          "cwd": "${workspaceRoot}",
          "runtimeExecutable": "${workspaceRoot}/electron/electron",
          "env": {
            // "ELECTRON_FORCE_IS_PACKAGED": "true"
          },
          "args": ["app/app", "--disable-gpu", "--enable-source-maps"],
          // "program": "${workspaceRoot}/app/app/index.js",
          "outFiles": [
            "${workspaceFolder}/app/app/**/*.(m|c|)js",
            "!**/node_modules/**"
          ],
          "sourceMapPathOverrides": {
            "meteor://ðŸ’»app/*": "${workspaceFolder}/*",
            "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
            "webpack://?:*/*": "${workspaceFolder}/*"
          },
          "outputCapture": "std"
          
      }
  ]
}