{
  "name": "Bilibili Extension",
  "description": "哔哩哔哩漫游",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "page/login.js",
        "page/home.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}